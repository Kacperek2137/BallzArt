pico-8 cartridge // http://www.pico-8.com
version 29
__lua__

--init
x=0
y=64
angle=0
cls()

function _update60()

     --increment
     y+=sin(angle * 0.5)
     angle+=0.01
     angle = angle / 1.1

     --draw
     pset (x,y,7)

     x += 1
     --wrap x 
     if x > 128 then
	     cls()
	     x = 0
     end
end

